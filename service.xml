<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>EnableTransactions</key>
  <true/>
  <key>Label</key>
  <string>homebrew.mxcl.bind</string>
  <key>RunAtLoad</key>
  <true/>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/local/opt/bind/sbin/named</string>
    <string>-f</string>
    <string>-c</string>
    <string>/usr/local/etc/bind/named.conf</string>
  </array>
  <key>ServiceIPC</key>
  <false/>
</dict>
</plist>
